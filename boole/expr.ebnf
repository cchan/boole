LPAREN: "("
RPAREN: ")"
NOT: "!" | "~" | "\neg"
AND: "&" | "\wedge"
OR: "|" | "\vee"
IMP: "=>" | "\implies"
IFF: "<=>" | "\iff"
LITERAL: /[a-zA-Z]+/

?literal:   LITERAL                 -> on_literal
?iffexpr:   impexpr
          | impexpr IFF iffexpr     -> on_iff
?impexpr:   orexpr
          | orexpr IMP impexpr      -> on_imp
?orexpr:    andexpr
          | andexpr OR orexpr       -> on_or
?andexpr:   notexpr
          | notexpr AND andexpr     -> on_and
?notexpr:   parenexpr
          | NOT parenexpr           -> on_not
?parenexpr: literal
          | LPAREN iffexpr RPAREN   -> on_paren

?boolexpr:  iffexpr                 -> on_result

%import common.WS
%ignore WS
