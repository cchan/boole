LPAREN: "("
RPAREN: ")"
NOT: "!" | "~" | "\neg"
AND: "&" | "\wedge"
OR: "|" | "\vee"
IMP: "=>" | "\implies"
IFF: "<=>" | "\iff"
LITERAL: /[a-zA-Z]+/

iffexpr: impexpr IFF iffexpr | impexpr
impexpr: orexpr IMP impexpr | orexpr
orexpr: andexpr OR orexpr | andexpr
andexpr: notexpr AND andexpr | notexpr
notexpr: NOT parenexpr | parenexpr
parenexpr: LPAREN iffexpr RPAREN | LITERAL

boolexpr: iffexpr

%import common.WS
%ignore WS
